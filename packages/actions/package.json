{
  "name": "@mstack/actions",
  "version": "0.0.0",
  "type": "module",
  "module": "index.ts",
  "main": "index.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "svelte": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "vite build && svelte-kit sync && svelte-package --input src",
    "dev": "svelte-package --watch --input src",
    "lint": "prettier --check . && eslint . --max-warnings=0",
    "lint:fix": "prettier --write . && eslint --fix .",
    "prepare": "svelte-kit sync"
  },
  "svelte": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "!dist/**/*.test.*",
    "!dist/**/*.spec.*"
  ],
  "devDependencies": {
    "@mstack/tailwind-config": "*",
    "@mstack/typescript-config": "*",
    "@sveltejs/kit": "2.5.18",
    "@sveltejs/package": "2.3.2",
    "@sveltejs/vite-plugin-svelte": "4.0.0-next.4"
  },
  "dependencies": {
    "svelte": "5.0.0-next.192"
  }
}
